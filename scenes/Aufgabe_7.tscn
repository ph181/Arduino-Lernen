text = "void setup() {
  pinMode(11, OUTPUT);
  pinMode(12, POUTUT);
  pinMode(13, OUTPUT);
  pinMode(9, OUTPUT);
  pinMode(10, OUTPUT);
}
void loop() {
  ampelZyklus();
  fussgangerZyklus();}"
