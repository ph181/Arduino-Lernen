[node name="text" type="Label" parent="Frame2/Exer/Aufgabe"]
text = "void setup() {
  pinMode(11, OUTPUT);
  pinMode(12, POUTUT);
  pinMode(13, OUTPUT);
  pinMode(9, OUTPUT);
  pinMode(10, OUTPUT);
}
void loop() {
  ampelZyklus();
  fussgangerZyklus();}"

[node name="text2" type="Label" parent="Frame2/Exer/Aufgabe"]
text = "void ampelZyklus() {
  digitalWrite(11, HIGH);
  delay(1000);
  digitalWrite(12, HIGH);
  delay(500);
  digitalWrite(11, LOW);
  digitalWrite(12, LOW);
  digitalWrite(13, HIGH);
  delay(1000);
  digitalWrite(13, LOW); }
void fussgangerZyklus() {
  digitalWrite(10, HIGH);
  delay(500);
  digitalWrite(10, LOW);
  digitalWrite(9, HIGH);
  delay(1000);
  digitalWrite(9, LOW);
}"

